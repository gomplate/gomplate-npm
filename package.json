    "url": "https://github.com/hairyhenderson/gomplate/releases/download/v{{version}}/gomplate_{{platform}}-{{arch}}{{win_ext}}"
  },
  "dependencies": {
    "go-npm": "RecuencoJones/go-npm#ce647cb47f7825464a55425620e026968662b693"
  }
}
